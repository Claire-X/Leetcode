class Solution {
public:
    int numDecodings(string s) {
       if(!s.size()||s[0]=='0') return 0;
       int n = s.size();
       int f[n];
       fill_n(f,n,0);
       f[0] = 1;
       for(int i =1;i<n;i++){
            if(s[i]!='0')
            	f[i] = f[i-1];
            if(s[i-1]=='1'||s[i-1]=='2'&&s[i]<='6'&&s[i]>='0')
                f[i] = (i==1?f[i]+1:f[i] + f[i-2]);
                
       }
       return f[n-1];
    }
};
